<%= form_for @instructor do |f|%>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :qualification %>
<%= f.text_field :qualification %>
<%= f.label :instrument %>
<%= f.text_field :instrument %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :photo %>
<%= f.url_field :photo %>
<%= f.label :likes %>
<%= f.number_field :likes %>
<%= f.submit %>
<% end %>
